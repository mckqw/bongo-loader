{
  "name": "bongo-loader",
  "private": true,
  "@rollingversions/ignore": true,
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pugjs/pug.git"
  },
  "scripts": {
    "build": "browserify -t brfs -t browserify-css export.js -o pug.js "
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "./scripts/filename-serializer.js",
      "./scripts/prettier-javascript-serializer.js",
      "./scripts/buffer-serializer.js"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "3.0.9",
    "jest": "^26.0.1",
    "prettier": "1.19.1"
  },
  "dependencies": {
    "browserify-css": "^0.15.0",
    "brfs": "^2.0.2",
    "fs": "^0.0.1-security",
    "wsrun": "^5.2.0"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  }
}
